{
  "name": "generate_number",
  "description": "Blueprint para checar se números são maiores ou menores que 50",
  "blueprint_spec": {
    "lanes": [
      {
        "id": "listNumber",
        "name": "list_number",
        "rule": ["fn", ["&", "args"], true]
      }
    ],
    "nodes": [
      {
        "id": "START",
        "type": "Start",
        "name": "Iniciar o processo Generate number",
        "parameters": {
          "input_schema": {},
          "timeout": 3600
        },
        "next": "GET-NUMBER",
        "lane_id": "listNumber"
      },
      {
        "id": "GET-NUMBER",
        "name": "Gerar numero",
        "next": "CHECK-NUMBER",
        "type": "SystemTask",
        "category": "setToBag",
        "lane_id": "listNumber",
        "parameters": {
          "input": {
            "number": {
              "$js": "() => { function generateNumber(min, max) { return Math.random() * (max - min) + min;} return (parseInt(generateNumber(0, 100)))}"
            }
          }
        }
      },
      {
        "id": "CHECK-NUMBER",
        "type": "Flow",
        "name": "Checar se número é maior que 50",
        "lane_id": "listNumber",
        "next": {
          "true": "NOTIFY-NUMERO-MAIOR-QUE-50",
          "default": "NOTIFY-NUMERO-MENOR-QUE-50"
        },
        "parameters": {
          "input": {
            "number": {
              "$js": "({ bag }) => {  return (bag.number > 50) }"
            }
          }
        }
      },
      {
        "id": "NOTIFY-NUMERO-MAIOR-QUE-50",
        "name": "Notificar usuário que número é maior que 50",
        "lane_id": "listNumber",
        "next": "MAIOR-QUE-50",
        "type": "UserTask",
        "parameters": {
          "input": {},
          "action": "SHOW_ALERT_MAIOR",
          "activity_manager": "notify"
        }
      },
      {
        "id": "MAIOR-QUE-50",
        "type": "Flow",
        "name": "Checar se número é maior que 50",
        "lane_id": "listNumber",
        "next": {
          "true": "PETIT-GATEAU-MAIOR-QUE-50",
          "default": "NOTIFY-NUMERO-MAIOR-QUE-50"
        },
        "parameters": {
          "input": {
            "number": {
              "$js": "({ bag }) => {  return (bag.number > 50) }"
            }
          }
        }
      },
      {
        "id": "NOTIFY-NUMERO-MENOR-QUE-50",
        "name": "USER_TASK",
        "lane_id": "listNumber",
        "next": "PETIT-GATEAU-MENOR-QUE-50",
        "type": "UserTask",
        "parameters": {
          "input": {},
          "action": "SHOW_ALERT_MENOR",
          "activity_manager": "notify"
        }
      },
      {
        "id": "PETIT-GATEAU-MENOR-QUE-50",
        "type": "Finish",
        "name": "FINISH_NODE",
        "lane_id": "listNumber",
        "next": null,
        "parameters": {
          "input": {}
        }
      },
      {
        "id": "PETIT-GATEAU-MAIOR-QUE-50",
        "type": "Finish",
        "name": "FINISH_NODE",
        "lane_id": "listNumber",
        "next": null,
        "parameters": {
          "input": {}
        }
      }
    ],
    "prepare": [],
    "environment": {},
    "requirements": ["core"]
  }
}
