{
  "name": "prego",
  "main": "./src/index.js",
  "type": "module",
  "version": "6.1.0",
  "description": "Package to handle quiver-based structures",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quivero/quivero-api.git"
  },
  "keywords": [
    "data-structures",
    "algorithms"
  ],
  "author": "brunolnetto",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/quivero/quivero-api/issues"
  },
  "homepage": "https://github.com/quivero/quivero-api#readme",
  "scripts": {
    "build": "npm run node:build && npm run docker:build",
    "node:build": "./utils/sscripts/node-build.sh",
    "docker:build": "./utils/sscripts/docker-build.sh",
    "docker:deploy": "./utils/sscripts/docker-deploy.sh $npm_config_tag $npm_config_from $npm_config_to",
    "docker:watch": "./utils/sscripts/docker-deploy.sh $npm_config_tag",
    "update": "bash ./utils/sscripts/update.sh",
    "start": "nodemon --trace-warnings --experimental-modules --es-module-specifier-resolution=node ./src/index.js",
    "test": "jest",
    "test:watch": "jest --no-cache --watch",
    "test:ci": "jest --no-cache --collect-coverage --coverage --ci",
    "release": "np --any-branch",
    "nibble": "npx eslint-nibble .",
    "fixable-nibble": "npx eslint-nibble --fixable-only .",
    "format": "prettier --write . && prettier --check .",
    "prepare": "husky install"
  },
  "dependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.2",
    "axios": "^1.1.3",
    "clean": "^4.0.2",
    "date-and-time": "^2.4.1",
    "esm": "^3.2.25",
    "express": "^4.18.2",
    "express-status-monitor": "^1.3.4",
    "fs": "^0.0.2",
    "husky": "^8.0.2",
    "jest-light-runner": "^0.4.1",
    "lodash": "^4.17.21",
    "lodash.multicombinations": "^1.0.0",
    "lodash.multipermutations": "^1.0.0",
    "mermaid": "^9.2.2",
    "morgan": "^1.10.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "react-scripts": "^2.1.3",
    "rimraf": "^3.0.2",
    "sinon": "^15.0.0",
    "strongly-connected-components": "^1.0.1",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.19.1",
    "@babel/plugin-transform-runtime": "7.19.6",
    "@babel/preset-env": "7.20.2",
    "@matteoh2o1999/github-actions-jest-reporter": "^1.0.2",
    "@microsoft/eslint-formatter-sarif": "^3.0.0",
    "@types/jest": "28.1.8",
    "@typescript-eslint/eslint-plugin": "5.46.1",
    "@typescript-eslint/parser": "5.46.1",
    "coveralls": "^3.1.1",
    "eslint-nibble": "8.1.0",
    "eslint-plugin-jest": "^27.1.7",
    "eslint-plugin-jest-extended": "^2.0.0",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-standard": "^5.0.0",
    "jest": "29.3.1",
    "jest-progress-bar-reporter": "^1.0.25",
    "jest-slow-test-reporter": "^1.0.0",
    "jest-snapshot": "^29.3.1",
    "lodash.combinations": "18.11.1",
    "node-notifier": "^10.0.1",
    "nodemon": "^2.0.20",
    "np": "^3.0.4",
    "npm-check-engine": "1.0.0",
    "webpack": "5.75.0",
    "webpack-cli": "5.0.1"
  },
  "engines": {
    "node": " >=16.0.0",
    "npm": ">=6.9.0"
  }
}
