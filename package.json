{
  "name": "dot-quiver",
  "type": "module",
  "version": "5.4.1",
  "description": "Package to handle dot-quiver based structures",
  "main": "./src/index.js",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brunolnetto/dot-quiver.git"
  },
  "keywords": [
    "data-structures",
    "algorithms"
  ],
  "author": "brunolnetto",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brunolnetto/dot-quiver/issues"
  },
  "homepage": "https://github.com/brunolnetto/dot-quiver#readme",
  "dependencies": {
    "@babel/preset-env": "^7.16.11",
    "eslint-nibble": "^8.0.1",
    "express": "^4.17.3",
    "fs": "^0.0.2",
    "lodash": "^4.17.21",
    "nodemon": "^2.0.15",
    "np": "^7.6.0",
    "strongly-connected-components": "^1.0.1",
    "typescript": "^4.6.2"
  },
  "scripts": {
    "start": "nodemon ./src/index.js",
    "test": "jest --coverage",
    "test:watch": "jest --watchAll --coverage",
    "test:ci": "jest --ci --reporters='default' --reporters='./github-actions-reporter'",
    "publish": "np --any-branch",
