    "@typescript-eslint/parser": "5.16.0",
    "eslint": "^8.11.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-scope": "^7.1.1",
    "jest": "^27.5.1",
    "lodash.combinations": "^18.10.0",
    "npm-check-engine": "^1.0.0",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  },
  "engines": {
    "node": " >=16.0.0",
    "npm": ">=6.9.0"
