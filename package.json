{
  "name": "quivero-api",
  "main": "./src/index.js",
  "type": "module",
  "version": "6.0.4",
  "description": "Package to handle dot-quiver-based structures",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quivero/quivero-api.git"
  },
  "keywords": [
    "data-structures",
    "algorithms"
  ],
  "author": "brunolnetto",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/quivero/quivero-api/issues"
  },
  "homepage": "https://github.com/quivero/quivero-api#readme",
  "dependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.19.1",
    "axios": "^1.0.0",
    "clean": "^4.0.2",
    "date-and-time": "^2.4.1",
    "esm": "^3.2.25",
    "express": "^4.18.1",
    "express-status-monitor": "^1.3.4",
    "fs": "^0.0.2",
    "lodash": "^4.17.21",
    "lodash.multicombinations": "^1.0.0",
    "lodash.multipermutations": "^1.0.0",
    "mermaid": "^8.4.3",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "react-scripts": "^2.1.3",
    "rimraf": "^3.0.2",
    "sinon": "^14.0.1",
    "strongly-connected-components": "^1.0.1",
    "winston": "^3.8.2"
  },
  "scripts": {
    "build": "npm run node:build && npm run docker:build", 
    "node:build": "./utils/sscripts/node-build.sh",
    "systemctl:watch": "journalctl -u quivero.service",
    "systemctl:deploy": "./utils/sscripts/systemctl-deploy.sh",
    "systemctl:restart": "systemctl restart quivero.service",
    "docker:build": "./utils/sscripts/docker-build.sh",
    "docker:deploy": "./utils/sscripts/docker-deploy.sh $npm_config_tag $npm_config_from $npm_config_to",
    "docker:watch": "./utils/sscripts/docker-deploy.sh $npm_config_tag",
    "update": "bash ./utils/sscripts/update.sh",
    "start": "nodemon ./src/index.js",
    "test:run": "jest --coverage",
    "test:watch": "jest --watchAll --coverage",
    "test:ci": "jest --ci --reporters='default' --reporters='./github-actions-reporter'",
    "publish": "np --any-branch",
    "nibble": "npx eslint-nibble .",
    "fixable-nibble": "npx eslint-nibble --fixable-only ."
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.19.1",
    "@babel/plugin-transform-runtime": "7.19.6",
    "@babel/preset-env": "7.19.4",
    "@types/jest": "28.1.8",
    "@typescript-eslint/eslint-plugin": "5.41.0",
    "@typescript-eslint/parser": "5.41.0",
    "eslint-nibble": "8.1.0",
    "jest": "28.1.3",
    "lodash.combinations": "18.11.1",
    "np": "3.1.0",
    "npm-check-engine": "1.0.0",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0"
  },
  "engines": {
    "node": " >=16.0.0",
    "npm": ">=6.9.0"
  }
}
