{
  "name": "prego",
  "type": "module",
  "imports": {
    "#utils/*": "./utils/*.js",
    "#utils/*.js": "./utils/*.js",
    "#gutils/*": "./data-structures/graph/utils/*.js",
    "#gutils/*.js": "./data-structures/graph/utils/*.js",
    "#algorithms/*": "./algorithms/*.js",
    "#algorithms/*.js": "./algorithms/*.js",
    "#dstructures/*": "./data-structures/*.js",
    "#dstructures/*.js": "./data-structures/*.js",
    "#galgorithms/*": "./data-structures/graph/algorithms/*.js",
    "#galgorithms/*.js": "./data-structures/graph/algorithms/*.js"
  },
  "version": "6.1.0",
  "description": "Package to handle quiver-based structures",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quivero/prego.git"
  },
  "keywords": [
    "data-structures",
    "algorithms"
  ],
  "author": "brunolnetto",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/quivero/prego/issues"
  },
  "homepage": "https://github.com/quivero/prego#readme",
  "scripts": {
    "build": "npm run node:build && npm run docker:build",
    "node:build": "./utils/sscripts/node-build.sh",
    "docker:build": "./utils/sscripts/docker-build.sh",
    "docker:deploy": "./utils/sscripts/docker-deploy.sh $npm_config_tag $npm_config_from $npm_config_to",
    "docker:watch": "./utils/sscripts/docker-deploy.sh $npm_config_tag",
    "update": "bash ./utils/sscripts/update.sh",
    "start": "nodemon --trace-warnings --experimental-modules ./src/index.js",
    "test:clear_cache": "jest  --clearCache",
    "test": "npm run test:clear_cache && jest",
    "test:watch": "jest --watchAll",
    "test:ci": "jest --collect-coverage --coverage --ci",
    "release": "np --any-branch",
    "nibble": "npx eslint-nibble .",
    "fixable-nibble": "npx eslint-nibble --fixable-only .",
    "format": "prettier --write . && prettier --check .",
    "prepare": "husky install"
  },
  "dependencies": {
    "clean": "^4.0.2",
    "esm": "^3.2.25",
    "express": "^4.18.2",
    "express-status-monitor": "^1.3.4",
    "fs": "^0.0.2",
    "husky": "^8.0.2",
    "lodash": "^4.17.21",
    "lodash.multicombinations": "^1.0.0",
    "mermaid": "^9.3.0",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "prettier": "^2.8.1",
    "rimraf": "^4.0.0",
    "strongly-connected-components": "^1.0.1",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "7.19.6",
    "@babel/preset-env": "7.20.2",
    "@matteoh2o1999/github-actions-jest-reporter": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "5.48.1",
    "@typescript-eslint/parser": "5.48.1",
    "dotenv": "^16.0.3",
    "eslint-nibble": "8.1.0",
    "eslint-plugin-jest": "^27.1.7",
    "eslint-plugin-jest-extended": "^2.0.0",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "29.3.1",
    "jest-progress-bar-reporter": "^1.0.25",
    "jest-slow-test-reporter": "^1.0.0",
    "lodash.combinations": "18.11.1",
    "nodemon": "^2.0.20",
    "np": "^3.0.4",
    "npm-check-engine": "1.0.0",
    "webpack": "5.75.0"
  },
  "engines": {
    "node": " >=16.0.0",
    "npm": ">=6.9.0"
  }
}
